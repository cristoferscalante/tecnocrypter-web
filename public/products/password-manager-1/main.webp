<svg width="500" height="300" viewBox="0 0 500 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="passwordBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#7c2d12;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#dc2626;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="safeGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#374151;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1f2937;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="500" height="300" fill="url(#passwordBg)"/>
  
  <!-- Safe/Vault -->
  <rect x="180" y="80" width="140" height="120" rx="10" fill="url(#safeGradient)" stroke="#6b7280" stroke-width="3"/>
  
  <!-- Safe Door -->
  <circle cx="250" cy="140" r="35" fill="#4b5563" stroke="#9ca3af" stroke-width="2"/>
  <circle cx="250" cy="140" r="25" fill="#374151"/>
  <circle cx="250" cy="140" r="15" fill="#1f2937"/>
  
  <!-- Safe Handle -->
  <rect x="285" y="135" width="20" height="10" rx="5" fill="#fbbf24" stroke="#f59e0b" stroke-width="1"/>
  
  <!-- Keys floating around -->
  <g transform="translate(120, 100) rotate(15)">
    <rect x="0" y="0" width="25" height="8" rx="4" fill="#fbbf24"/>
    <rect x="20" y="-3" width="8" height="3" fill="#fbbf24"/>
    <rect x="20" y="8" width="8" height="3" fill="#fbbf24"/>
    <circle cx="5" cy="4" r="3" fill="none" stroke="#fbbf24" stroke-width="2"/>
  </g>
  
  <g transform="translate(350, 120) rotate(-20)">
    <rect x="0" y="0" width="25" height="8" rx="4" fill="#10b981"/>
    <rect x="20" y="-3" width="8" height="3" fill="#10b981"/>
    <rect x="20" y="8" width="8" height="3" fill="#10b981"/>
    <circle cx="5" cy="4" r="3" fill="none" stroke="#10b981" stroke-width="2"/>
  </g>
  
  <g transform="translate(140, 180) rotate(45)">
    <rect x="0" y="0" width="20" height="6" rx="3" fill="#3b82f6"/>
    <rect x="16" y="-2" width="6" height="2" fill="#3b82f6"/>
    <rect x="16" y="6" width="6" height="2" fill="#3b82f6"/>
    <circle cx="4" cy="3" r="2" fill="none" stroke="#3b82f6" stroke-width="1.5"/>
  </g>
  
  <!-- Title -->
  <text x="250" y="40" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="24" font-weight="bold">CryptoPass Manager</text>
  
  <!-- Subtitle -->
  <text x="250" y="60" text-anchor="middle" fill="#fca5a5" font-family="Arial, sans-serif" font-size="14">Secure Password Management</text>
  
  <!-- Features -->
  <text x="250" y="230" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="12">🔐 Zero-Knowledge Encryption</text>
  <text x="250" y="245" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="12">🔑 2FA Authentication</text>
  <text x="250" y="260" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="12">☁️ Secure Cloud Sync</text>
  <text x="250" y="275" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="12">🚨 Security Alerts</text>
</svg>