<svg width="500" height="300" viewBox="0 0 500 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="generatorBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e1b4b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3730a3;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="500" height="300" fill="url(#generatorBg)"/>
  
  <!-- Generator Window -->
  <rect x="50" y="40" width="400" height="220" rx="10" fill="#1e293b" stroke="#475569" stroke-width="2"/>
  
  <!-- Title -->
  <text x="250" y="70" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="18" font-weight="bold">Password Generator</text>
  
  <!-- Generated Password Display -->
  <rect x="70" y="85" width="360" height="40" rx="5" fill="#0f172a" stroke="#374151" stroke-width="1"/>
  <text x="250" y="110" text-anchor="middle" fill="#10b981" font-family="Courier, monospace" font-size="14" font-weight="bold">Kx9#mP2$vL8@nQ5!</text>
  
  <!-- Copy Button -->
  <rect x="380" y="90" width="40" height="30" rx="3" fill="#3b82f6" stroke="#2563eb" stroke-width="1"/>
  <text x="400" y="108" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="10">Copy</text>
  
  <!-- Length Slider -->
  <text x="80" y="150" fill="#ffffff" font-family="Arial, sans-serif" font-size="12">Length: 16</text>
  <rect x="80" y="160" width="300" height="4" rx="2" fill="#374151"/>
  <rect x="80" y="160" width="200" height="4" rx="2" fill="#10b981"/>
  <circle cx="280" cy="162" r="8" fill="#10b981"/>
  
  <!-- Options Checkboxes -->
  <g transform="translate(80, 180)">
    <!-- Uppercase -->
    <rect x="0" y="0" width="12" height="12" rx="2" fill="#10b981" stroke="#059669" stroke-width="1"/>
    <path d="M2 6 L5 9 L10 3" stroke="#ffffff" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
    <text x="20" y="10" fill="#ffffff" font-family="Arial, sans-serif" font-size="11">Uppercase (A-Z)</text>
    
    <!-- Lowercase -->
    <rect x="150" y="0" width="12" height="12" rx="2" fill="#10b981" stroke="#059669" stroke-width="1"/>
    <path d="M152 6 L155 9 L160 3" stroke="#ffffff" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
    <text x="170" y="10" fill="#ffffff" font-family="Arial, sans-serif" font-size="11">Lowercase (a-z)</text>
  </g>
  
  <g transform="translate(80, 200)">
    <!-- Numbers -->
    <rect x="0" y="0" width="12" height="12" rx="2" fill="#10b981" stroke="#059669" stroke-width="1"/>
    <path d="M2 6 L5 9 L10 3" stroke="#ffffff" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
    <text x="20" y="10" fill="#ffffff" font-family="Arial, sans-serif" font-size="11">Numbers (0-9)</text>
    
    <!-- Symbols -->
    <rect x="150" y="0" width="12" height="12" rx="2" fill="#10b981" stroke="#059669" stroke-width="1"/>
    <path d="M152 6 L155 9 L160 3" stroke="#ffffff" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
    <text x="170" y="10" fill="#ffffff" font-family="Arial, sans-serif" font-size="11">Symbols (!@#$%)</text>
  </g>
  
  <!-- Exclude Similar -->
  <g transform="translate(80, 220)">
    <rect x="0" y="0" width="12" height="12" rx="2" fill="#374151" stroke="#6b7280" stroke-width="1"/>
    <text x="20" y="10" fill="#94a3b8" font-family="Arial, sans-serif" font-size="11">Exclude similar characters (0, O, l, 1)</text>
  </g>
  
  <!-- Strength Indicator -->
  <text x="350" y="150" fill="#ffffff" font-family="Arial, sans-serif" font-size="12">Strength:</text>
  <rect x="350" y="160" width="60" height="8" rx="4" fill="#374151"/>
  <rect x="350" y="160" width="50" height="8" rx="4" fill="#10b981"/>
  <text x="350" y="180" fill="#10b981" font-family="Arial, sans-serif" font-size="10" font-weight="bold">Very Strong</text>
  
  <!-- Generate Button -->
  <rect x="320" y="200" width="100" height="35" rx="5" fill="#dc2626" stroke="#b91c1c" stroke-width="1"/>
  <text x="370" y="222" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Generate New</text>
</svg>