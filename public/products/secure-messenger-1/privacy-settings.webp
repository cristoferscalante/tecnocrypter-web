<svg width="500" height="300" viewBox="0 0 500 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="privacyBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e1b4b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3730a3;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="500" height="300" fill="url(#privacyBg)"/>
  
  <!-- Settings Window -->
  <rect x="50" y="30" width="400" height="240" rx="10" fill="#1e293b" stroke="#475569" stroke-width="2"/>
  
  <!-- Header -->
  <rect x="50" y="30" width="400" height="35" rx="10" fill="#3730a3"/>
  <text x="250" y="52" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Privacy & Security Settings</text>
  
  <!-- Settings Content -->
  <g transform="translate(70, 80)">
    <!-- End-to-End Encryption -->
    <rect x="0" y="0" width="360" height="30" rx="5" fill="#334155"/>
    <circle cx="15" cy="15" r="8" fill="#10b981"/>
    <text x="15" y="20" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">🔒</text>
    <text x="35" y="12" fill="#ffffff" font-family="Arial, sans-serif" font-size="12" font-weight="bold">End-to-End Encryption</text>
    <text x="35" y="24" fill="#94a3b8" font-family="Arial, sans-serif" font-size="9">All messages are encrypted by default</text>
    <!-- Toggle ON -->
    <rect x="320" y="8" width="30" height="14" rx="7" fill="#10b981"/>
    <circle cx="337" cy="15" r="6" fill="#ffffff"/>
    
    <!-- Auto Identity Verification -->
    <rect x="0" y="40" width="360" height="30" rx="5" fill="#475569"/>
    <circle cx="15" cy="55" r="8" fill="#3b82f6"/>
    <text x="15" y="60" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">✓</text>
    <text x="35" y="52" fill="#ffffff" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Automatic Identity Verification</text>
    <text x="35" y="64" fill="#94a3b8" font-family="Arial, sans-serif" font-size="9">Verify contacts automatically using cryptographic keys</text>
    <!-- Toggle ON -->
    <rect x="320" y="48" width="30" height="14" rx="7" fill="#10b981"/>
    <circle cx="337" cy="55" r="6" fill="#ffffff"/>
    
    <!-- Message Auto-Destruction -->
    <rect x="0" y="80" width="360" height="30" rx="5" fill="#334155"/>
    <circle cx="15" cy="95" r="8" fill="#dc2626"/>
    <text x="15" y="100" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">💥</text>
    <text x="35" y="92" fill="#ffffff" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Message Auto-Destruction</text>
    <text x="35" y="104" fill="#94a3b8" font-family="Arial, sans-serif" font-size="9">Set timer: 30 seconds, 5 minutes, 1 hour, 1 day</text>
    <!-- Dropdown -->
    <rect x="290" y="88" width="60" height="14" rx="3" fill="#6b7280"/>
    <text x="320" y="98" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="8">5 minutes ▼</text>
    
    <!-- Metadata Protection -->
    <rect x="0" y="120" width="360" height="30" rx="5" fill="#475569"/>
    <circle cx="15" cy="135" r="8" fill="#fbbf24"/>
    <text x="15" y="140" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">🛡️</text>
    <text x="35" y="132" fill="#ffffff" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Metadata Protection</text>
    <text x="35" y="144" fill="#94a3b8" font-family="Arial, sans-serif" font-size="9">Hide message timestamps and read receipts</text>
    <!-- Toggle ON -->
    <rect x="320" y="128" width="30" height="14" rx="7" fill="#10b981"/>
    <circle cx="337" cy="135" r="6" fill="#ffffff"/>
    
    <!-- Screenshot Protection -->
    <rect x="0" y="160" width="360" height="30" rx="5" fill="#334155"/>
    <circle cx="15" cy="175" r="8" fill="#8b5cf6"/>
    <text x="15" y="180" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">📱</text>
    <text x="35" y="172" fill="#ffffff" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Screenshot Protection</text>
    <text x="35" y="184" fill="#94a3b8" font-family="Arial, sans-serif" font-size="9">Prevent screenshots and screen recording</text>
    <!-- Toggle OFF -->
    <rect x="320" y="168" width="30" height="14" rx="7" fill="#6b7280"/>
    <circle cx="327" cy="175" r="6" fill="#ffffff"/>
  </g>
  
  <!-- Bottom Info -->
  <rect x="70" y="240" width="360" height="20" rx="5" fill="#3730a3" opacity="0.8"/>
  <text x="250" y="253" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="10">🔐 Your privacy is our priority - No data is stored on our servers</text>
</svg>